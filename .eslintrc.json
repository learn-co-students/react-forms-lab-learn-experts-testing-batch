{
  "extends": ["google", "eslint:recommended", "plugin:react/recommended"],


  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "strict": 0
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },

  "env": {
    "node": true,
    "mocha": true,
    "jasmine": true,
    "phantomjs":true
  }


}
